{
  "name": "docue",
  "version": "0.1.1",
  "engines": {
    "node": "22.x"
  },
  "private": true,
  "scripts": {
    "prestart": "./node_modules/.bin/migrate",
    "start": "nodemon index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "NODE_ENV='test' npm run coverage",
    "test:simple": "NODE_ENV='test' node spec/run.js",
    "test:parallel": "NODE_ENV='test' MONGO_TEST_URL=mongodb://localhost/docue-test-$$ node spec/run.js",
    "coverage": "nyc node spec/run.js && nyc report --reporter=text-lcov | coveralls",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.3.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "migrate": "^0.2.2",
    "moment": "^2.30.1",
    "mongoose": "^8.9.3",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.9",
    "underscore": "^1.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@eslint/js": "^9.39.1",
    "coveralls": "^2.11.6",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "jasmine": "^5.5.0",
    "mockgoose": "^7.3.3",
    "nyc": "^11.3.0",
    "supertest": "^2.0.1"
  }
}
